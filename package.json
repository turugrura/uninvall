		"@types/passport-local": "^1.0.36",
		"@types/supertest": "^2.0.14",
		"@types/validator": "^13.11.3",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
